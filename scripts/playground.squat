struct Person {
    int age;
    int height;
    string name;
}

func add2Persons(Person person1, Person person2) Person {
    return Person(person1.age + person2.age, person1.height + person2.height, person1.name + person2.name);
}

func main() {
    Person person1 = Person(25, 175, "person1");
    Person person2 = Person(20, 180, "person2");

    Person addedPerson = add2Persons(person1, person2);
    println(addedPerson);
}
