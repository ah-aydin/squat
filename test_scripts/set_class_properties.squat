class Person {
    int age;
    int height;
    string name;
}

Person global_guy = Person(9, 123, "global");

func main() {
    Person person = Person(29, 120, "short guy");
    int a = 0;
    int b = 1;

    a = b = person.age;
    println(a);
    println(b);

    println("Before change");
    println(person);
    
    person.age = 39;
    person.height = 199;
    person.name = "now dis guy is tall guy";

    println("After change");
    println(person);

    person.age = a = b = 320;
    println(person.age);
    println(a);
    println(b);
    println(person);

    person.name = "new name";
    println(person);

    println(global_guy);
    global_guy.name = "ofya";
    println(global_guy);
    person.name = global_guy.name;
    println(person);
    person.name = global_guy.name = "last name";
    println(person);
    println(global_guy);
}

