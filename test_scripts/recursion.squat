func fib(int depth) int {
    if (depth == 0) {
        return 0;
    }
    if (depth == 1) {
        return 1;
    }
    var value = fib(depth - 1) + fib(depth - 2);
    return fib(depth - 1) + fib(depth - 2);
}

func main() {
    var fib_location = 10;
    print("Fibonaci number at location ");
    print(fib_location);
    print(" is ");
    print(fib(fib_location));
}
